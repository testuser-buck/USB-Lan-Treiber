;; Windows 8
;;****************************************************************************
[RTL8152B.ndi.NT]
AddReg = RTLUE8023W8.NT.Reg
AddReg = About.reg,
AddReg = Common.reg,
AddReg = NetworkAddress.reg, Speed10_100.reg, wolptnmagic.reg, WolShutdownLinkSpeed.reg
